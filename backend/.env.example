# Google AI
GOOGLE_API_KEY=your-gemini-api-key

# Redis Cloud (Hot Cache + Semantic Cache)
# Get from Redis Cloud console: https://cloud.redis.io
REDIS_URL=redis://default:password@your-redis-host:port
REDIS_INDEX_NAME=prompttriage_hot

# Pinecone (Full Corpus Vector Store)
# Get from Pinecone console: https://app.pinecone.io
PINECONE_API_KEY=your-pinecone-api-key
PINECONE_INDEX_NAME=prompttriage-prompts
PINECONE_ENVIRONMENT=us-east-1  # or your region

# LangCache (Semantic Caching) - Optional
# Get from: https://langcache.redis.io
LANGCACHE_URL=https://your-instance.langcache.redis.io
LANGCACHE_API_KEY=your-langcache-api-key

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:3000

# Embedding model (sentence-transformers)
EMBEDDING_MODEL=all-mpnet-base-v2
